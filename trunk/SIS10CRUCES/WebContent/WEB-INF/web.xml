<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:web="http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd" id="WebApp_ID" version="3.0">
  <display-name>SIS10CRUCES</display-name>
  <welcome-file-list>
    <welcome-file>index.jsp</welcome-file>
  </welcome-file-list>
  <servlet>
    <servlet-name>login</servlet-name>
    <servlet-class>pe.plazanorte.sisterra.seguridad.Login</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>login</servlet-name>
    <url-pattern>/Login</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>usuarios</servlet-name>
    <servlet-class>pe.plazanorte.sisterra.seguridad.Usuarios</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>usuarios</servlet-name>
    <url-pattern>/Usuarios</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>agregar</servlet-name>
    <servlet-class>pe.plazanorte.sisterra.seguridad.AgregarUsuario</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>agregar</servlet-name>
    <url-pattern>/Agregar</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>servletProveedor</servlet-name>
    <servlet-class>pe.plazanorte.sisterra.proveedor.ServletProveedor</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>servletProveedor</servlet-name>
    <url-pattern>/ServletProveedor</url-pattern>
  </servlet-mapping>
</web-app>